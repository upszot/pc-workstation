[commands]
as_command0 = mate-calc
as_command1 = caja
as_run_command0_key = <Alt><Super>c

[vpswitch]
as_switch_to_1_key = <Super>1
as_switch_to_2_key = <Super>2
as_switch_to_3_key = <Super>3
as_switch_to_4_key = <Super>4

[core]
as_active_plugins = core;commands;vpswitch;svg;screenshot;png;mousepoll;water;dbus;ezoom;text;resize;place;decoration;minimize;wobbly;grid;glib;matecompat;ring;shift;fade;cube;scale;opacify;obs;move;rotate;switcher;3d;
as_autoraise_delay = 100
as_unmaximize_window_key = <Alt><Super>Down
as_maximize_window_key = <Alt><Super>Up
as_window_menu_key = Disabled
as_show_desktop_key = <Control><Primary><Alt>m
as_toggle_window_shaded_key = Disabled
s0_outputs = 1366x768+0+0;

[ring]
as_next_key = Disabled
as_prev_key = Disabled

[wall]
as_up_window_key = Disabled
as_down_window_key = Disabled

[shift]
as_next_key = Disabled
as_prev_key = Disabled
as_next_all_key = <Super>Tab
as_prev_all_key = <Shift><Super>Tab

[3d]
s0_max_window_space = 15

[cube]
s0_multioutput_mode = 2
s0_acceleration = 3.000000
s0_speed = 1.000000
s0_active_opacity = 56.000000

[scale]
as_initiate_key = Disabled
as_initiate_all_key = <Shift><Alt>Up

[opacify]
as_toggle_key = Disabled

[decoration]
as_command = gtk-window-decorator --replace

[grid]
as_put_center_key = Disabled
as_put_left_key = <Super>Left
as_put_right_key = <Super>Right
as_put_top_key = <Super>Up
as_put_bottom_key = <Super>Down
as_put_topleft_key = Disabled
as_put_topright_key = Disabled
as_put_bottomleft_key = Disabled
as_put_bottomright_key = Disabled
as_put_maximize_key = Disabled

[matecompat]
as_run_key = <Super>r
as_command_window_screenshot = mate-screenshot --interactive
as_command_terminal = terminator
as_run_command_terminal_key = <Alt><Super>t

